<Group labelreference="L238" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Equation executable="true" style="2D Input" input-equation="nodes2d := proc (N) local alpopt, alpha, Np, L1, L2, L3, sk, x, y, blend1, blend2, blend3, warpf1, warpf2, warpf3, n, m; alpopt := [0, 0, 1.4152, .1001, .2751, .98, 1.099, 1.2832, 1.3648, 1.4773, 1.4959, 1.5743, 1.5770, 1.6223, 1.6258]; if N &lt; 16 then alpha := alpopt(N) else alpha := 5/3 end if; Np := (1/2)*(N+1)*(N+2); L1 := [seq(0, i = 1 .. Np)]; L2 := [seq(0, i = 1 .. Np)]; L3 := [seq(0, i = 1 .. Np)]; sk := 1; for n to N+1 do for m to N+2-n do L1[sk] := (n-1)/N; L3[sk] := (m-1)/N; sk := sk+1 end do end do; L2 := [seq(1, i = 1 .. Np)]-L1-L3; x := -L2+L3; y := (-L2-L3+2*L1)/sqrt(3); blend1 := `~`[`*`](4*L2, L3); blend2 := `~`[`*`](4*L1, L3); blend3 := `~`[`*`](4*L1, L2); [blend1, blend2, blend3] end proc:" display=""></Equation></Text-field>
